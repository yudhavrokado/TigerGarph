  IF s.system_source_code == "PKADM" THEN s.@channel += "CORPO" END,
  IF s.system_source_code == "ASKES" THEN s.@channel += "CORPO" END;
  
  getpoliscertificateproduct = SELECT t FROM PolicyCertificate:t -(HasPolicyCertificate)- Policy:s
  ACCUM t.@product += s.@product, t.@premium_per_moth+= s.@premium_per_moth, t.@channel += s.@channel;
  
  result1 = SELECT t FROM Person:t -(HasPolicyInsured)- Policy:s
  ACCUM t.@product += s.@product, t.@count_policy += s.outdegree("HasPolicyInsured"), t.@channel += s.@channel, t.@premium_per_moth += s.@premium_per_moth,
  IF t.@count_policy > 1 THEN t.@max_insureddate += s.insurance_start_date, t.@min_insureddate += s.insurance_start_date
  ELSE IF t.@count_policy <= 1 THEN t.@max_insureddate += s.insurance_start_date, t.@min_insureddate += s.insurance_start_date END
  LIMIT 4;
  
  result2 = SELECT t FROM Person:t -(HasPolicyCertificateInsured)- PolicyCertificate:s
  ACCUM t.@product += s.@product, t.@count_policy += s.outdegree("HasPolicyCertificateInsured"), t.@channel += s.@channel, t.@premium_per_moth += s.@premium_per_moth,
  IF t.@count_policy > 1 AND s.nuke == "" THEN t.@max_insureddate += s.policy_start_date, t.@min_insureddate += s.policy_start_date END,
  IF t.@count_policy <= 1 AND s.nuke == "" THEN t.@max_insureddate += s.policy_start_date, t.@min_insureddate += s.policy_start_date END,
  IF t.@count_policy > 1 AND s.nuke == "001" THEN t.@max_insureddate += s.insurance_date, t.@min_insureddate += s.insurance_date END,
  IF t.@count_policy <= 1 AND s.nuke == "001" THEN t.@max_insureddate += s.insurance_date, t.@min_insureddate += s.insurance_date END
  LIMIT 4;
 
  
  PRINT result1, result2;
}