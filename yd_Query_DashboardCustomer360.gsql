CREATE DISTRIBUTED QUERY yd_Query_DashboardCustomer360() FOR GRAPH ifgl { 
  MaxAccum<STRING> @master, @id_number, @address, @gender, @no_hp, @email, @pob, @product, @channel, @generasi;
  MaxAccum<INT> @age;
  MaxAccum<DOUBLE> @premium_per_moth;
  MinAccum<DATETIME> @min_insureddate;
  MaxAccum<DATETIME> @max_insureddate, @dob;
  SumAccum<INT> @count_policy, @count_child = 0;
  
  getmasterperson = SELECT t FROM Person:s -(MergedFrom>:e)- Person:t
  ACCUM t.@master += s.person_id, 
  IF e.type == "MergedFrom" THEN t.@count_child += 1 END
  ;
  
  mulai = SELECT s FROM Person:s -((HasPersonID|HasPermanentAddress|HasPersonGender|HasPersonPOB|HasPersonDOB|HasPermanentEmail|HasPermanentPhone|HasOthersAddress|HasWorkAddress|HasOthersEmail|HasBillingEmail|HasHomePhone|HasMobilePhone|HasBillingPhone|HasOthersPhone|HasWorkPhone|HasFax):e)- :t
  ACCUM
  CASE 
    WHEN e.type =="HasPersonID" THEN s.@id_number += t.id_number
    WHEN e.type =="HasPermanentAddress" THEN s.@address += t.address_name
    WHEN e.type =="HasOthersAddress" THEN s.@address += t.address_name
    WHEN e.type =="HasWorkAddress" THEN s.@address += t.address_name
    WHEN e.type =="HasPersonGender" THEN s.@gender += t.gender
    WHEN e.type =="HasPersonPOB" THEN s.@pob += t.place_of_birth
    WHEN e.type =="HasPersonDOB" THEN s.@dob += t.date_of_birth, s.@age += (datetime_diff(now(),t.date_of_birth))/31536000,
      CASE
        WHEN (year(t.date_of_birth) BETWEEN 1946 AND 1964) THEN s.@generasi += "Baby Boomer"
        WHEN (year(t.date_of_birth) BETWEEN 1965 AND 1980) THEN s.@generasi += "Generasi X"
        WHEN (year(t.date_of_birth) BETWEEN 1981 AND 1996) THEN s.@generasi += "Millennials"
        WHEN (year(t.date_of_birth) BETWEEN 1997 AND 2012) THEN s.@generasi += "Generasi Z"
        WHEN year(t.date_of_birth) >= 2013 THEN s.@generasi += "Generasi Alpha"
      END
    WHEN e.type =="HasPermanentPhone" THEN s.@no_hp += t.phone_number
    WHEN e.type =="HasHomePhone" THEN s.@no_hp += t.phone_number
    WHEN e.type =="HasWorkPhone" THEN s.@no_hp += t.phone_number
    WHEN e.type =="HasMobilePhone" THEN s.@no_hp += t.phone_number
    WHEN e.type =="HasOthersPhone" THEN s.@no_hp += t.phone_number
    WHEN e.type =="HasFax" THEN s.@no_hp += t.phone_number
    WHEN e.type =="HasPermanentEmail" THEN s.@email += t.email_name
    WHEN e.type =="HasOthersEmail" THEN s.@email += t.email_name
    WHEN e.type =="HasBillingEmail" THEN s.@email += t.email_name
  END;
    
  getpremiumpolicy = SELECT s FROM Policy:s -(HasPolicyCoverage)- Coverage:t
  ACCUM 
  IF t.premium_amount > 0  THEN s.@premium_per_moth += t.premium_amount END,
  IF t.premium_amount <= 0 THEN s.@premium_per_moth += 0 END;
  
  getpolisproduk = SELECT s FROM Policy:s -(HasPolicyProduct)- Product:t
  ACCUM s.@product += t.product_name,
  IF t.product_name LIKE "%BTN%" THEN s.@channel += "BANCASS" END;
  
  getpolischannel = SELECT s FROM Policy:s
  POST-ACCUM
  IF s.system_source_code == "NADM" AND s.agent_id != "" THEN s.@channel += "AGENCY" END,
  IF s.agent_id == "0000074807" THEN s.@channel += "WORKSITE" END,
  IF s.agent_id == "0000074798" THEN s.@channel += "WORKSITE" END,
  IF s.agent_id == "0000751320" THEN s.@channel += "WORKSITE" END,
  IF s.agent_id == "0000074799" THEN s.@channel += "WORKSITE" END,
